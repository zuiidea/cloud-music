{
    "src": {
        "css": [
            "src/css/**/*.less"
        ],
        "cssPub": [
          "src/lib/framework7.material.min.css",
          "src/lib/framework7.material.colors.min.css",
          "src/css/app.less"
        ],
        "js": [
            "src/js/**/*.js"
        ],
        "jsPub": [
            "src/lib/*.js",
            "src/js/**/*.js"
        ],
        "html": [
            "src/**/*.html"
        ],
        "img": [
            "src/img/**/*.*"
        ],
        "lib": [
            "src/lib/*.*"
        ],
        "clean": "dist/**/*.*"
    },
    "dest": {
        "css": "dist/css/",
        "img": "dist/img/",
        "js": "dist/js/",
        "html": "dist/",
        "lib": "dist/lib/",
        "module": "dist/module/"
    }
}
