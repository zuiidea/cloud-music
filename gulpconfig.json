{
    "src": {
        "css": [
            "src/css/**/*.less"
        ],
        "cssEntry": "src/css/app.less",
        "js": [
            "src/js/**/*.js",
            "!src/js/framework7.min.js"
        ],
        "html": [
            "src/html/**/*.html"
        ],
        "img": [
            "src/img/**/*.*"
        ],
        "lib": [
            "src/lib/*.*"
        ],
        "clean": "dist/**/*.*"
    },
    "dest": {
        "css": "dist/css/",
        "img": "dist/img/",
        "js": "dist/js/",
        "html": "dist/html/",
        "lib": "dist/lib/",
        "module": "dist/module/"
    }
}
